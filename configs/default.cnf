[mysqld]
local_infile = 1
character_set_server = utf8
init-connect = 'SET NAMES utf8'
collation_server = utf8_general_ci
sync_binlog = 1
sql_mode = STRICT_TRANS_TABLES,STRICT_ALL_TABLES,NO_ZERO_IN_DATE,NO_ZERO_DATE,ERROR_FOR_DIVISION_BY_ZERO,TRADITIONAL,NO_AUTO_CREATE_USER,NO_ENGINE_SUBSTITUTION,ONLY_FULL_GROUP_BY
